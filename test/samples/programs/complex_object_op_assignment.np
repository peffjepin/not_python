class A:
    value: int

    def __iadd__(self, other: int) -> A:
        self.value += other
        return self


class B:
    a: A

b = B(A(1))
b.a += 1
print(b)

l1 = [A(1), A(2), A(3)]
l1[0] += 100
print(l1[0], l1[1], l1[2])

l2 = [B(A(1)), B(A(2)), B(A(3))]
l2[0].a += 100
print(l2[0], l2[1], l2[2])
